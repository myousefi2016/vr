<xml>
    <proxy>
        <group>filters</group>
        <name>GeodesicMeasurement</name>
        <label>Geodesic Measurement</label>
        <documentation>
            <brief/>
            <long/>
        </documentation>
        <property>
            <name>Input</name>
            <label>Input</label>
            <defaults/>
            <domains>
                <domain>
                    <text>Accepts input of following types:</text>
                    <list>
                        <item>vtkPolyData</item>
                    </list>
                </domain>
            </domains>
        </property>
        <property>
            <name>Endpoints</name>
            <label>Endpoints</label>
            <documentation>
                <brief/>
                <long>The input that provides the endpoints between
        which geodesic paths will be computed.
        </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>Accepts input of following types:</text>
                    <list>
                        <item>vtkPointSet</item>
                    </list>
                </domain>
            </domains>
        </property>
        <property>
            <name>Loop</name>
            <label>Create Loop</label>
            <documentation>
                <brief/>
                <long>
          If on, enables loop creation by computing a path from the
          last point to first point in the endpoints.
        </long>
            </documentation>
            <defaults>0</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>LoopWithLine</name>
            <label>Create Loop with Line</label>
            <documentation>
                <brief/>
                <long>
          If on and if Create Loop is on, uses a straight line segment
          to connect the last point to the first point in the endpoints.
        </long>
            </documentation>
            <defaults>0</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
    </proxy>
    <proxy>
        <group>filters</group>
        <name>FastMarchingGeodesicDistanceField</name>
        <label>Fast-Marching Geodesic Distance-Field From Binary Field</label>
        <documentation>
            <brief>Compute the distance of points of the mesh to chosen seeds</brief>
            <long>
          Compute the distance of each point of the mesh to a set of seeds
          defined as points with non-zero value. Internal Fast-Marching-Method
          plugin is used for computational efficiency.
      </long>
        </documentation>
        <property>
            <name>Input</name>
            <label>Input</label>
            <defaults/>
            <domains>
                <domain>
                    <text>Accepts input of following types:</text>
                    <list>
                        <item>vtkPolyData</item>
                    </list>
                </domain>
                <domain>
                    <text>
      The dataset must contain a field array ()
      
        with 1 component(s).
      </text>
                </domain>
                <domain>
                    <text>
      The dataset must contain a field array ()
      
        with 1 component(s).
      </text>
                </domain>
            </domains>
        </property>
        <property>
            <name>SeedsNonZeroField</name>
            <label>Seeds Non-Zero Field</label>
            <documentation>
                <brief/>
                <long>
        Select the input array to be treated as defining seeds.
        A point having a non-zero value will be considered as being a seed.
        </long>
            </documentation>
            <defaults>0</defaults>
            <domains>
                <domain>
                    <text>An array of scalars is required.</text>
                </domain>
                <domain>
                    <text>The value must be field array name.</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>IsotropicMetricTensorLength</name>
            <label>IsotropicMetricTensorLength</label>
            <documentation>
                <brief/>
                <long>Select the input array to be treated as the Metric
        Tensor.It can only be an isometric tensor, and the selected field should
        contain the length of the metric tensor at each polydata point.
        </long>
            </documentation>
            <defaults>1</defaults>
            <domains>
                <domain>
                    <text>An array of scalars is required.</text>
                </domain>
                <domain>
                    <text>The value must be field array name.</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>OutputFieldName</name>
            <label>OutputFieldName</label>
            <documentation>
                <brief/>
                <long>
        Set the output field name.
        </long>
            </documentation>
            <defaults>DistanceField</defaults>
            <domains/>
        </property>
    </proxy>
</xml>
