<xml>
    <proxy>
        <group>sources</group>
        <name>H5PartReader</name>
        <label>H5PartReader</label>
        <documentation>
            <brief/>
            <long>
        Reader for H5Part (HDF5) particle files.
      </long>
        </documentation>
        <property>
            <name>FileName</name>
            <label>FileName</label>
            <defaults/>
            <domains>
                <domain>
                    <text> The value(s) must be a filename (or filenames).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>MaskOutOfTimeRangeOutput</name>
            <label>MaskOutOfTimeRangeOutput</label>
            <defaults>0</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>TimestepValues</name>
            <label>TimestepValues</label>
            <defaults/>
            <domains/>
        </property>
        <property>
            <name>Xarray</name>
            <label>Xarray</label>
            <documentation>
                <brief/>
                <long>
         This property lists which point array to use for coordinates.
       </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>The value(s) can be one of the following:</text>
                    <list/>
                </domain>
            </domains>
        </property>
        <property>
            <name>Yarray</name>
            <label>Yarray</label>
            <documentation>
                <brief/>
                <long>
         This property lists which point array to use for coordinates.
       </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>The value(s) can be one of the following:</text>
                    <list/>
                </domain>
            </domains>
        </property>
        <property>
            <name>Zarray</name>
            <label>Zarray</label>
            <documentation>
                <brief/>
                <long>
         This property lists which point array to use for coordinates.
       </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>The value(s) can be one of the following:</text>
                    <list/>
                </domain>
            </domains>
        </property>
        <property>
            <name>PointArrayInfo</name>
            <label>PointArrayInfo</label>
            <defaults/>
            <domains/>
        </property>
        <property>
            <name>PointArrayStatus</name>
            <label>Point Arrays</label>
            <documentation>
                <brief/>
                <long>
         This property lists which point-centered arrays to read.
       </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>The list of array names is provided by the reader.</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>GenerateVertexCells</name>
            <label>GenerateVertexCells</label>
            <defaults>1</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
        <ReaderFactory extensions="h5part" file_description="H5Part particle files (Plugin)"/>
    </proxy>
    <categoryindex>
        <label>Readers</label>
        <item proxy_name="H5PartReader" proxy_group="sources"/>
    </categoryindex>
</xml>
