<xml>
    <proxy>
        <group>filters</group>
        <name>ArrowGlyphFilter</name>
        <label>ArrowGlyphFilter</label>
        <documentation>
            <brief>A customized Glyph filter for arrows only</brief>
            <long/>
        </documentation>
        <property>
            <name>Input</name>
            <label>Input</label>
            <documentation>
                <brief/>
                <long>
          This property specifies the input to the filter.
          This is the dataset to which the glyphs will be applied.
        </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>Accepts input of following types:</text>
                    <list>
                        <item>vtkDataSet</item>
                    </list>
                </domain>
                <domain>
                    <text>
      The dataset must contain a field array ()
      </text>
                </domain>
                <domain>
                    <text>
      The dataset must contain a field array (point)
      
        with 3 component(s).
      </text>
                </domain>
                <domain>
                    <text>
      The dataset must contain a field array (point)
      
        with 1 component(s).
      </text>
                </domain>
            </domains>
        </property>
        <property>
            <name>ArrowSourceObject</name>
            <label>ArrowSourceObject</label>
            <defaults/>
            <domains>
                <domain>
                    <text>The value can be one of the following:</text>
                    <list>
                        <item>ArrowSource (sources)
        </item>
                    </list>
                </domain>
            </domains>
        </property>
        <property>
            <name>OrientationVectorArray</name>
            <label>Orientation</label>
            <documentation>
                <brief/>
                <long>
          This (mandatory) property indicates the name of the Velocity array.
        </long>
            </documentation>
            <defaults>direction</defaults>
            <domains>
                <domain>
                    <text>An array of vectors is required.</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>ScaleByOrientationVectorMagnitude</name>
            <label>ScaleByOrientationVectorMagnitude</label>
            <documentation>
                <brief/>
                <long>
          Apply a scaling accroding to the vector magnitude
        </long>
            </documentation>
            <defaults>1</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>ScaleArray</name>
            <label>Scaling Array</label>
            <documentation>
                <brief/>
                <long>
          The array to use for scaling glyphs
        </long>
            </documentation>
            <defaults>Scale</defaults>
            <domains/>
        </property>
        <property>
            <name>ScaleFactor</name>
            <label>ScaleFactor</label>
            <documentation>
                <brief/>
                <long>
          Apply a fixed scale to the glyphs
        </long>
            </documentation>
            <defaults>1.0</defaults>
            <domains>
  Value(s) must be in the range (
    ,
    ).
</domains>
        </property>
        <property>
            <name>ShaftRadiusArray</name>
            <label>Shaft Radius Scaling Array</label>
            <documentation>
                <brief/>
                <long>
          The array to use for scaling shaft radius
        </long>
            </documentation>
            <defaults>Scale</defaults>
            <domains/>
        </property>
        <property>
            <name>ShaftRadiusFactor</name>
            <label>ShaftRadiusFactor</label>
            <documentation>
                <brief/>
                <long>
          Apply a fixed scale to the shaft radius
        </long>
            </documentation>
            <defaults>1.0</defaults>
            <domains>
  Value(s) must be in the range (
    ,
    ).
</domains>
        </property>
        <property>
            <name>TipRadiusArray</name>
            <label>Tip Radius Scaling Array</label>
            <documentation>
                <brief/>
                <long>
          The array to use for scaling tip radius
        </long>
            </documentation>
            <defaults>Scale</defaults>
            <domains/>
        </property>
        <property>
            <name>TipRadiusFactor</name>
            <label>TipRadiusFactor</label>
            <documentation>
                <brief/>
                <long>
          Apply a fixed scale to the tip radius
        </long>
            </documentation>
            <defaults>1.0</defaults>
            <domains>
  Value(s) must be in the range (
    ,
    ).
</domains>
        </property>
        <property>
            <name>MaximumNumberOfPoints</name>
            <label>Maximum Number of Points</label>
            <documentation>
                <brief/>
                <long>
          The value of this property specifies the maximum number of glyphs that should appear in the output dataset if the value of the UseMaskPoints property is 1. (See the UseMaskPoints property.)
        </long>
            </documentation>
            <defaults>5000</defaults>
            <domains>
  Value(s) must be in the range (
    0,
    ).
</domains>
        </property>
        <property>
            <name>UseMaskPoints</name>
            <label>Mask Points</label>
            <documentation>
                <brief/>
                <long>
          If the value of this property is set to 1, limit the maximum number of glyphs to the value indicated by MaximumNumberOfPoints. (See the MaximumNumberOfPoints property.)
        </long>
            </documentation>
            <defaults>1</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>RandomMode</name>
            <label>RandomMode</label>
            <documentation>
                <brief/>
                <long>
          If the value of this property is 1, then the points to glyph are chosen randomly. Otherwise the point ids chosen are evenly spaced.
        </long>
            </documentation>
            <defaults>1</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
    </proxy>
</xml>
